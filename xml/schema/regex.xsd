<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:element name="gender">
          <xsd:simpleType>
             <xsd:restriction base="xsd:string">   
             	<xsd:pattern value="male|female"/>
              </xsd:restriction>
            </xsd:simpleType>
    </xsd:element>
</xsd:schema>